    <Method Name="GetNextFrame" Id="{26e244c8-5196-4630-95f7-ff67685fbedb}">
      <Declaration><![CDATA[METHOD GetNextFrame : ARRAY[0..255] OF UDINT
]]></Declaration>
      <Implementation>
        <ST><![CDATA[FOR I := 0 TO 255 DO
	GetNextFrame[I] := ThisVideo[CurrentFrame,I];
END_FOR

IF CurrentFrame = TotalFramesInFilm - 1 THEN
	CurrentFrame := 0;
ELSE
	CurrentFrame := CurrentFrame + 1;
END_IF
]]></ST>
      </Implementation>
    </Method>
